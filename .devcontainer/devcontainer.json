{
    "name": "bb-remote-execution",
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "remoteEnv": {
        "PATH": "${containerWorkspaceFolder}:${containerEnv:PATH}"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "bazelbuild.vscode-bazel",
                "golang.go",
                "Anthropic.claude-code",
                "mhutchie.git-graph"
            ]
        }
    },
    "remoteUser": "vscode"
}
